{
  "src": ".",
  "out": "${annoMods}/${modName}",  
  "converter": [
    {
      "action": "static",
      "pattern": "**/*.{cfg,dds,prp}"
    },
    {
      "action": "static",
      "pattern": "{data/config/**/*,banner.png,README.md}"
    },
    {
      "action": "texture",
      "pattern": "**/*_{diff,norm,rga,height,metal,mask}.png",
      "lods": 3,
      "changePath": "maps/"
    },
    {
      "action": "cfgyaml",
      "pattern": "**/*.cfg.yaml"
    },
    {
      "action": "modinfo",
      "content_en": true
    }
  ],
  "modinfo": {
    "Version": "1.2",
    "ModID": "jakob_ground_textures",
    "IncompatibleIds": [],
    "ModDependencies": [],
    "Category": {
      "English": "Shared"
    },
    "ModName": {
      "Chinese": null,
      "English": "Ground Textures",
      "French": null,
      "German": "Bodentexturen",
      "Italian": null,
      "Japanese": null,
      "Korean": null,
      "Polish": null,
      "Russian": null,
      "Spanish": null,
      "Taiwanese": null
    },
    "Description": {
      "English": "README.md",
      "German": "README.md"
    },
    "KnownIssues": null,
    "DLCDependencies": [],
    "CreatorName": "Jakob",
    "CreatorContact": "https://github.com/jakobharder/anno-1800-jakobs-mods",
    "Image" : null
  }
}