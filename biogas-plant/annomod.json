{
  "src": ".",
  "out": "${annoMods}/${modName}",  
  "converter": [
    {
      "action": "static",
      "pattern": "**/*.{cfg,ifo,prp,fc,rdm,dds}"
    },
    {
      "action": "static",
      "pattern": "{data/config/**/*,**/icons/*.png,banner.png,README.md}"
    },
    {
      "action": "cf7",
      "pattern": "**/*.cf7"
    },
    {
      "action": "texture",
      "pattern": "**/*_{diff,norm,height,metal}.png"
    },
    {
      "action": "gltf",
      "pattern": "**/*.{glb,gltf}"
    },
    {
      "action": "modinfo",
      "content_en": true
    }
  ],
  "modinfo": {
    "Version": "1.1",
    "ModID": "jakob_Biogas_Plant",
    "IncompatibleIds": [],
    "ModDependencies": null,
    "Category": {
      "English": "Gameplay"
    },
    "ModName": {
      "English": "Small Power Plants, Biogas",
      "German": "Kleine Kraftwerke, Biogas"
    },
    "Description": {
      "English": "README.md"
    },
    "KnownIssues": null,
    "DLCDependencies": null,
    "CreatorName": "jakob",
    "CreatorContact": null,
    "Image" : null
  }
}