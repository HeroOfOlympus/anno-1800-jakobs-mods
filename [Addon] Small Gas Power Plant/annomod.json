{
  "src": ".",
  "out": "${annoMods}/${modName}",  
  "converter": [
    {
      "action": "static",
      "pattern": "**/*.{cfg,ifo,prp,fc,rdm,dds}"
    },
    {
      "action": "static",
      "pattern": "{data/config/**/*,banner.png,README.md,**/effects/*.png}"
    },
    {
      "action": "cf7",
      "pattern": "**/*.cf7"
    },
    {
      "action": "rdpxml",
      "pattern": "**/*.rdp.xml"
    },
    {
      "action": "texture",
      "pattern": "**/icons/*.png",
      "lods": 0
    },
    {
      "action": "texture",
      "pattern": "**/*_{diff,norm,height,metal}.png",
      "lods": 3,
      "changePath": "maps/"
    },
    {
      "action": "gltf",
      "pattern": "**/*.{glb,gltf}",
      "lods": 4,
      "changePath": "rdm/",
      "animPath": "anim/"
    },
    {
      "action": "modinfo",
      "content_en": true
    }
  ],
  "modinfo": {
    "Version": "1.1.1",
    "ModID": "jakob_Small_Gas_Power_Plant",
    "IncompatibleIds": [],
    "ModDependencies": null,
    "Category": {
      "English": "Gameplay"
    },
    "ModName": {
      "Chinese": "小型燃气发电厂",
      "English": "Small Gas Power Plant",
      "French": "Petit centrale au gaz",
      "German": "Kleines Gaskraftwerk",
      "Italian": "Piccola centrale elettrica a gas",
      "Japanese": "小型ガス火力発電所",
      "Korean": "소형 가스 화력 발전소",
      "Polish": "Mała elektrownia gazowa",
      "Russian": "Малая газовая электростанция",
      "Spanish": "Pequeña central eléctrica a gas",
      "Taiwanese": "小型燃氣發電廠"
    },
    "Description": {
      "English": "README.md",
      "German": "README_german.md"
    },
    "KnownIssues": null,
    "DLCDependencies": [
      {
        "DLC": "ThePassage",
        "Dependant": "required"
      }
    ],
    "CreatorName": "Jakob",
    "CreatorContact": "https://github.com/jakobharder/anno-1800-jakobs-mods",
    "Image" : null
  }
}