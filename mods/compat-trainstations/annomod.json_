{
  "src": ".",
  "out": "${annoMods}/${modName}",  
  "converter": [
    {
      "action": "static",
      "pattern": "**/*.{cfg,ifo,prp,fc,rdm,dds}"
    },
    {
      "action": "static",
      "pattern": "{data/config/**/*,**/icons/*.png,banner.png,README.md}"
    },
    {
      "action": "cfgyaml",
      "pattern": "**/*.cfg.yaml"
    },
    {
      "action": "modinfo",
      "content_en": true
    }
  ],
  "modinfo": {
    "Version": "1",
    "ModID": "jakob_compatfix_hotels",
    "IncompatibleIds": [],
    "ModDependencies": null,
    "Category": {
      "English": ""
    },
    "ModName": {
      "English": "Train-Stations, Make compatible with NW Tourism",
      "German": "Train-Stations, Make compatible with NW Tourism"
    },
    "Description": {
      "English": "README.md",
      "German": "README.md"
    },
    "KnownIssues": null,
    "DLCDependencies": [ ],
    "CreatorName": "Jakob",
    "CreatorContact": "https://github.com/jakobharder/anno-1800-jakobs-mods",
    "Image" : null
  }
  
}